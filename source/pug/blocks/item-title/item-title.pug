mixin item-title(data)
    +el({
        tag: 'h3',
        class: 'item-title',
        mod: data.mod,
        mix: data.mix,
        js: data.js,
        text: data.text,
    })&attributes(attributes)
        if data.descr
            +el({
                tag: 'p',
                class: 'item-title__descr',
                text: data.descr,
            })
